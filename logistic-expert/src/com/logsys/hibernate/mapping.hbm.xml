<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!-- 程序总Hibernate Mapping文件 -->

<hibernate-mapping package="com.logsys.demand">

	<class name="DemandContent" table="demand">
		<id name="id" column="id">
			<generator class="increment"/>
		</id>
		<property name="pn" type="string" column="pn" />
		<property name="qty" type="double" column="qty"/>
		<property name="date" type="date" column="date" />
		<property name="dlvfix" type="int" column="dlvfix" />
	</class>

</hibernate-mapping>

<hibernate-mapping package="com.logsys.model">

	<class name="ModelContent" table="model">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="client" type="string" column="clinet" />
		<property name="model" type="string" column="model" />
		<property name="prjcode" type="string" column="prjcode" />
		<property name="info" type="string" column="info" />
	</class>

</hibernate-mapping>