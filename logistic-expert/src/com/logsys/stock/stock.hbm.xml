<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.logsys.stock">

	<class name="StockContent" table="stock">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="date" type="timestamp" column="date" />
		<property name="pn" type="string" column="pn" />
		<property name="plant" type="string" column="plant" />
		<property name="sloc" type="string" column="sloc" />
		<property name="wkcenter" type="string" column="wkcenter" />
		<property name="qty" type="double" column="qty" />
		<property name="uom" type="string" column="uom" />
		<property name="status" type="string" column="status" />
	</class>
	
</hibernate-mapping>