<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.logsys.model">

	<class name="ModelContent" table="model">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="client" type="string" column="client" />
		<property name="model" type="string" column="model" />
		<property name="prjcode" type="string" column="prjcode" />
		<property name="info" type="string" column="info" />
		<property name="pq" type="integer" column="pq" />
		<property name="mrpfactor" type="double" column="mrpfactor"/>
		<property name="mrpround" type="int" column="mrpround" />
		<property name="intranday" type="int" column="intranday" />
	</class>

</hibernate-mapping>