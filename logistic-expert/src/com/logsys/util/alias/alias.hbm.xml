<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.logsys.util.alias">

	<class name="AliasContent" table="alias">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="aliastype" type="string" column="aliastype" />
		<property name="stdname" type="string" column="stdname" />
		<property name="aliasname" type="string" column="aliasname" />
	</class>

</hibernate-mapping>